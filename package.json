    }
  },
  "dependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "awesome-lint": "^0.12.1",
    "doctoc": "^1.4.0",
    "jshint": "^2.11.1",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "husky": "^4.2.5"
  }
}
